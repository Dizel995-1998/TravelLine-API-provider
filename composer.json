{
    "name": "egik/travelline-api",
    "version": "1.0.2",
    "description": "API client for bil24",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "doctrine/annotations": "^1.13",
        "friendsofphp/php-cs-fixer": "^3.8",
        "guzzlehttp/guzzle": "^7.0",
        "jworman/annotation-reader": "^1.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/cache": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/validator": "^5.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "egik\\TravellineApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "egik\\TravellineApi\\": "tests/"
        }
    },
    "scripts": {
        "test" : "./vendor/bin/simple-phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "vimeo/psalm": "^4.22"
    }
}
